{
  "name": "@qliaison/monorepo",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "clean": "rimraf -g \"./packages/*/lib\" \"./packages/*/*.tsbuildinfo\"",
    "build": "tsc --build",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier './packages/*/src/**/*.ts' --write",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lerna": "^6.6.2",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
