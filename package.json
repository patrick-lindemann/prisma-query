{
  "name": "qliaison",
  "version": "0.0.1",
  "description": "Effortlessly translate queries from various sources to flexible where clauses for popular ORM frameworks.",
  "author": "Patrick Lindemann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrick-lindemann/qliaison"
  },
  "keywords": [
    "filter",
    "query",
    "translator",
    "connector",
    "sequelize",
    "prisma",
    "ag-grid",
    "json"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "commonjs",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint src --quiet --fix",
    "prepack": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "grammar-build": "nearleyc src/dialects/query/grammar/grammar.ne -o src/dialects/query/grammar/grammar.ts && eslint --fix src/dialects/query/grammar/grammar.ts"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.23",
    "lodash.isempty": "^4.4.0",
    "nearley": "^2.20.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/lodash.isempty": "^4.4.7",
    "@types/nearley": "^2.11.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0"
  }
}
